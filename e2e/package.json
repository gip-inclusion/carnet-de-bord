{
	"name": "@cdb/e2e",
	"version": "1.0.0",
	"description": "e2e tests for carnet-de-bord",
	"repository": "git@github.com:SocialGouv/carnet-de-bord.git",
	"author": "Incubateur des Minist√®res Sociaux <dsi-incubateur@sg.social.gouv.fr> (https://incubateur.social.gouv.fr)",
	"bugs": "https://github.com/SocialGouv/carnet-de-bord/issues",
	"main": "codecept.conf.js",
	"license": "Apache-2.0",
	"scripts": {
		"lint": "pre-commit run --all-files",
		"lint:pre-commit": "pre-commit run --all-files",
		"lint:prettier": "prettier --config .prettierrc --check",
		"lint:prettier:all": "yarn lint:prettier src ./**/*.{svelte,cjs,js,json}",
		"lint:eslint": "eslint --config=.eslintrc.cjs",
		"lint:eslint:all": "yarn lint:eslint .",
		"fix": "yarn fix:prettier && yarn fix:eslint",
		"fix:prettier": "yarn lint:prettier --write",
		"fix:prettier:all": "yarn lint:prettier:all --write",
		"fix:eslint": "yarn lint:eslint --fix --cache",
		"fix:eslint:all": "yarn lint:eslint:all --fix --cache",
		"test": "LANGUAGE=fr codeceptjs run",
		"test:debug": "LANGUAGE=fr codeceptjs run --debug --steps --verbose",
		"test:ui": "LANGUAGE=fr CODECEPT_UI=true codecept-ui --app"
	},
	"dependencies": {
		"@codeceptjs/helper": "^2.0.0",
		"@codeceptjs/ui": "^0.4.6",
		"codeceptjs": "^3.3.0",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@socialgouv/eslint-config-recommended": "^1.109.0",
		"eslint": "^8.13.0",
		"playwright": "^1.27.0",
		"prettier": "^2.6.2"
	},
	"private": true
}
