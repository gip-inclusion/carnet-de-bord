{
	"name": "carnet-de-bord",
	"version": "1.129.3",
	"engines": {
		"node": "16"
	},
	"private": true,
	"repository": "git@github.com:SocialGouv/carnet-de-bord.git",
	"author": "Incubateur des Minist√®res Sociaux <dsi-incubateur@sg.social.gouv.fr> (https://incubateur.social.gouv.fr)",
	"bugs": "https://github.com/SocialGouv/carnet-de-bord/issues",
	"license": "Apache-2.0",
	"scripts": {
		"k8s": "yarn --silent --cwd .socialgouv",
		"hasura": "hasura",
		"hasura:console": "cd hasura && hasura console",
		"hasura:seed": "cd hasura && hasura seed apply --database-name carnet_de_bord",
		"hasura:migrate": "cd hasura && hasura migrate apply --database-name carnet_de_bord",
		"compose": "docker-compose up --build",
		"dev": "svelte-kit dev --host",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"svelte-check": "svelte-check --tsconfig ./tsconfig.json",
		"svelte-check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "pre-commit run --all-files && yarn svelte-check",
		"lint:pre-commit": "pre-commit run --all-files",
		"lint:prettier": "prettier --config .prettierrc --check src ./*.{svelte,cjs,js,json,md}",
		"lint:eslint": "eslint --config=.eslintrc.cjs --ignore-pattern e2e src .",
		"fix": "yarn fix:prettier && yarn fix:eslint",
		"fix:prettier": "yarn lint:prettier --write",
		"fix:eslint": "yarn lint:eslint --fix --cache",
		"format": "yarn fix:prettier",
		"codegen": "graphql-codegen --config codegen.cjs",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:experimental": "./scripts/launch_tests.sh"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.0.0",
		"@commitlint/config-conventional": "^17.0.0",
		"@graphql-codegen/cli": "^2.3.0",
		"@graphql-codegen/typed-document-node": "^2.2.1",
		"@graphql-codegen/typescript": "^2.4.1",
		"@graphql-codegen/typescript-operations": "^2.2.1",
		"@graphql-codegen/urql-svelte-operations-store": "^1.1.8",
		"@sveltejs/adapter-node": "^1.0.0-next.60",
		"@sveltejs/kit": "^1.0.0-next.215",
		"@tailwindcss/forms": "^0.5.0",
		"@tailwindcss/typography": "^0.5.0",
		"@testing-library/jest-dom": "^5.16.1",
		"@testing-library/svelte": "^3.0.3",
		"@types/cookie": "^0.5.0",
		"@types/jsonwebtoken": "^8.5.6",
		"@types/nodemailer": "^6.4.4",
		"@types/rasha": "^1.2.3",
		"@types/tailwindcss": "^3.0.0",
		"@types/testing-library__jest-dom": "^5.14.2",
		"@types/uuid": "^8.3.3",
		"@typescript-eslint/eslint-plugin": "^5.8.0",
		"@typescript-eslint/parser": "^5.8.0",
		"autoprefixer": "^10.4.0",
		"babel-preset-vite": "^1.0.4",
		"cssnano": "^5.0.14",
		"esbuild": "^0.14.8",
		"esbuild-jest": "^0.5.0",
		"eslint": "^8.13.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"hasura-cli": "^2.6.0",
		"isomorphic-fetch": "^3.0.0",
		"jest": "^28.0.0",
		"jest-environment-jsdom": "^28.1.0",
		"lint-staged": "^13.0.0",
		"postcss": "^8.4.5",
		"postcss-load-config": "^4.0.0",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.5.1",
		"remixicon": "^2.5.0",
		"svelte": "^3.44.3",
		"svelte-check": "^2.2.11",
		"svelte-jester": "^2.1.5",
		"svelte-preprocess": "^4.10.1",
		"tailwindcss": "^3.0.7",
		"ts-jest": "^28.0.0",
		"tslib": "^2.3.1",
		"typescript": "^4.5.4"
	},
	"dependencies": {
		"@babel/core": "^7.16.5",
		"@babel/preset-env": "^7.16.5",
		"@babel/preset-typescript": "^7.16.5",
		"@gouvfr/dsfr": "^1.6.0",
		"@urql/core": "^2.3.6",
		"@urql/svelte": "^1.3.3",
		"cookie": "^0.5.0",
		"date-fns": "^2.27.0",
		"dotenv": "^16.0.0",
		"fast-deep-equal": "^3.1.3",
		"fnv-plus": "^1.3.1",
		"graphql": "^16.2.0",
		"jsonwebtoken": "^8.5.1",
		"jwt-decode": "^3.1.2",
		"matomo-tracker": "^2.2.4",
		"nodemailer": "^6.7.2",
		"rasha": "^1.2.5",
		"svelecte": "^3.6.0",
		"svelte-accessible-dialog": "^2.1.3",
		"svelte-file-dropzone": "^0.0.15",
		"svelte-forms-lib": "^2.0.0",
		"uuid": "^8.3.2",
		"xlsx": "^0.18.5",
		"yup": "^0.32.11"
	},
	"type": "module",
	"lint-staged": {
		"*.{ts,js,cjs,mjs,svelte}": [
			"prettier --write --plugin-search-dir=.",
			"eslint --ignore-path .eslintignore --fix"
		],
		"*.{json,css,postcss,html,md}": [
			"prettier --write --plugin-search-dir=."
		]
	}
}
