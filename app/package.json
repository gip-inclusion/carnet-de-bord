		"svelte": "3.58.0",
		"svelte-check": "3.1.4",
		"svelte-kit-test-helpers": "1.0.1",
		"svelte-preprocess": "5.0.3",
		"tailwindcss": "^3.0.7",
		"tslib": "^2.3.1",
